{
  "name": "donutez_farm",
  "version": "0.0.1",
  "description": "Donutez Farm",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@taquito/michelson-encoder": "^9.0.1",
    "@taquito/rpc": "^9.0.1",
    "@taquito/signer": "^9.0.1",
    "@taquito/taquito": "^9.0.1",
    "child_process": "^1.0.2",
    "commander": "^7.2.0",
    "dotenv": "^10.0.0",
    "fs": "^0.0.1-security",
    "ganache-cli": "6.11.0-tezos.0",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "truffle": "5.5.0-tezos.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "postinstall": "patch-package",
    "clean": "rm -rf ./build ./output",
    "start-sandbox": "docker run --rm --name my-sandbox -e flextesa_node_cors_origin='*' --detach -p 8732:20000 tqtezos/flextesa:20210316 flobox start",
    "test": "yarn compile && yarn truffle test --bail",
    "compile-yf": "node scripts/cli.js build-yf --no-json -o contracts/compiled",
    "compile-FA12-token": "node scripts/cli.js build-FA12-token --no-json -o contracts/compiled",
    "compile-FA2-token": "node scripts/cli.js build-FA2-token --no-json -o contracts/compiled",
    "compile": "yarn clean && yarn truffle compile",
    "full-compile": "yarn clean && yarn compile-yf && yarn compile-FA12-token && yarn compile-FA2-token && yarn truffle compile",
    "migrate": "yarn clean && yarn truffle migrate",
    "migrate-florencenet": "yarn clean && yarn truffle migrate --network florencenet"
  },
  "keywords": [],
  "author": "Sergey Makov <makov.sergey.it@gmail.com> and Oleh Rubanik <geriys12@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smak0v/donutez_farm.git"
  }
}
